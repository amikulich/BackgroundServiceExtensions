{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ScheduledServices": {
    "CacheService": {
      "Enabled": true,
      "Expression": "*/1 * * * *" //runs every minute
    },
    "AdvancedCacheService": {
      "Enabled": true,
      "Expression": "* */2 * * *", //runs every 2 hours
      "CustomData":  "CustomData" 
    }
  } 
}
